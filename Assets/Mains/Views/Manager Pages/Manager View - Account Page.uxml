            </engine:VisualElement>
            <engine:VisualElement name="LanguageField" style="flex-direction: row; height: 100px;">
                <engine:VisualElement name="Icon" style="height: 75px; width: 75px; background-image: resource(&apos;Icons/Language&apos;); background-size: 75% 75%; align-self: center;" />
                <engine:Label text="Language" name="Text" class="label-field" style="margin-left: 10px; font-size: 40px; -unity-text-align: middle-left; flex-grow: 1;" />
                <engine:Label text="English" name="Text" class="label-field" style="margin-left: 10px; font-size: 40px; -unity-text-align: middle-left; color: rgb(222, 249, 93);" />
            </engine:VisualElement>
            <engine:VisualElement name="ThemeField" style="flex-direction: row; height: 100px;">
