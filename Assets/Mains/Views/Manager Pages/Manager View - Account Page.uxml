                <engine:Label text="Dark Theme" name="Text" class="label-field" style="margin-left: 10px; font-size: 40px; -unity-text-align: middle-left; color: rgb(222, 249, 93);" />
            </engine:VisualElement>
        </engine:VisualElement>
        <engine:VisualElement name="InformationsField" style="padding-left: 50px; padding-right: 50px; background-color: rgb(44, 44, 44); padding-top: 25px; padding-bottom: 25px; margin-bottom: 25px;">
            <engine:VisualElement name="LabelField" style="height: 75px; flex-direction: row; margin-bottom: 10px;">
                <engine:Label text="Informations" name="Label" class="label-field" style="-unity-font-style: bold; font-size: 45px; flex-grow: 1; -unity-text-align: middle-left;" />
